@model List<Song>
@{
    ViewData["Title"] = "Index";
}

<h1>Song Index</h1>
<br />
<br />
<a href="/Song/Create" class="btn btn-outline-success">New Song</a>
<br />
<br />
<table class="table table-hover table-bordered">
	<thead>
		<tr>
			<th>Id</th>
			<th>Title</th>
			<th>Author</th>
			<th>Information</th>



		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>
					@item.Id
				</td>
				<td>
					@item.Title
				</td>
				<td>
					@item.Author
				</td>
				<td>
					<a href="/Song/Details/@item.Id" class="btn btn-outline-info">Details</a>
					<a href="/Song/Update/@item.Id" class="btn btn-outline-secondary">Update</a>
					<a href="/Song/Delete/@item.Id" class="btn btn-outline-danger">Delete</a>
				</td>

			</tr>
		}

	</tbody>
</table>
