@model Album
@{
    ViewData["Title"] = "Details";
}

<h1>Album Details</h1>

<div class="card">
    <div class="card-body">
        <h4 class="card-title">@Model.Id</h4>
        <h4 class="card-title">@Model.Date</h4>
        <h4 class="card-title">@Model.Title</h4>

        <h4 class="card-title">@Model.MusicianId</h4>


        <a href="/Song/Index" class="btn btn-primary">Back To List</a>
    </div>
</div>


